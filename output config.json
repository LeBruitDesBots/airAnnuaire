{
  "outputs": [
    {
      "type": "table",
      "description": "Liste des sous actifs par nombre d'abonnés",
      "file_name": "out/par abos public sfw.md",
      "status_filter": [
        "PUBLIC",
        "RESTRICTED"
      ],
      "nsfw_filter": true,
      "sort_key": "subscriber_count",
      "sort_direction": "descending",
      "columns": [
        {
          "header": "Rang",
          "value": "{index}"
        },
        {
          "header": "Nom",
          "value": "[{name}](/r/{name})"
        },
        {
          "header": "Nombre d'abonnés",
          "value": "{subscriber_count}"
        }
      ]
    },

    {
      "type": "table",
      "description": "Liste des sous actifs par nombre d'abonnés",
      "file_name": "out/alpha tout.md",
      "status_filter": [
        "UNKNOWN",
        "PUBLIC",
        "RESTRICTED",
        "QUARANTINED",
        "PRIVATE",
        "BANNED",
        "DOESNT_EXIST"
      ],
      "nsfw_filter": false,
      "sort_key": "name",
      "sort_direction": "ascending",
      "columns": [
        {
          "header": "Nom",
          "value": "[{name}](/r/{name})"
        },
        {
          "header": "Nombre d'abonnés",
          "value": "{subscriber_count}"
        },
        {
          "header": "Statut",
          "value": "{status}"
        },
        {
          "header": "PSPLT",
          "value": "{is_nsfw}"
        }
      ]
    }
  ]
}
