{
  "outputs": [
    {
      "type": "table",
      "description": "Liste des sous actifs par nombre d'abonnés",
      "file_name": "par abonnés.md",
      "filters": [
        "ACTIVE"
      ],
      "sort_key": "subscriber_count",
      "sort_direction": "descending",
      "columns": [
        {
          "header": "Rang",
          "value": "{index} {({index-old_index}) if {index-old_index!=0}}"
        },
        {
          "header": "Nom",
          "value": "[{name}](/r/{name})"
        },
        {
          "header": "Nombre d'abonnés",
          "value": "{subscriber_count}"
        }
      ]
    },
    {
      "type": "table",
      "description": "Liste des sous publics par date de création",
      "file_name": "par date.md",
      "filters": [
        "ACTIVE",
        "INACTIVE",
        "EMPTY"
      ],
      "sort_key": "created_utc",
      "sort_direction": "ascending",
      "columns": [
        {
          "header": "Rang",
          "value": "{index}"
        },
        {
          "header": "Nom",
          "value": "[{name}](/r/{name})"
        },
        {
          "header": "Date de création",
          "value": "{datetime.fromtimestamp(timestamp).strftime('%Y-%m-%d')}"
        }
        {
          "header": "Nombre d'abonnés",
          "value": "{subscriber_count}"
        }
      ]
    },
    {
      "type": "table",
      "description": "Liste des sous existants par ordre alphabétique",
      "file_name": "alphabétique.md",
      "filters": [
        "ACTIVE",
        "INACTIVE",
        "EMPTY",
        "PRIVATE",
        "BANNED"
      ],
      "sort_key": "name",
      "sort_direction": "ascending",
      "columns": [
        {
          "header": "Nom",
          "value": "[{name}](/r/{name})"
        },
        {
          "header": "Statut",
          "value": "{status}"
        }
      ]
    },
    {
      "type": "table",
      "description": "Liste des noms traités",
      "file_name": "alphabétique.md",
      "filters": [
        "UNKNOWN",
        "ACTIVE",
        "INACTIVE",
        "EMPTY",
        "PRIVATE",
        "BANNED",
        "DOESNT_EXIST"
      ],
      "sort_key": "name",
      "sort_direction": "ascending",
      "columns": [
        {
          "header": "Nom",
          "value": "[{name}](/r/{name})"
        },
        {
          "header": "Statut",
          "value": "{status}"
        }
      ]
    }
  ]
}
